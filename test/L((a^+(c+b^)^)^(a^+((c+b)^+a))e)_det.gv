digraph finite_state_machine {
  rankdir=LR;  
  size="8,5"  label="(a*+(c+b*)*)*(a*+((c+b)*+a))e"  node [shape = doublecircle]; "4";
  node [shape = circle];
  __I__ [label="", style=invis, width=0]
  __I__ -> "0"
  "0" -> "1" [label = a];
  "0" -> "4" [label = e];
  "0" -> "2" [label = c];
  "0" -> "3" [label = b];
  "1" -> "1" [label = a];
  "1" -> "4" [label = e];
  "1" -> "2" [label = c];
  "1" -> "3" [label = b];
  "2" -> "1" [label = a];
  "2" -> "4" [label = e];
  "2" -> "2" [label = c];
  "2" -> "3" [label = b];
  "3" -> "1" [label = a];
  "3" -> "4" [label = e];
  "3" -> "2" [label = c];
  "3" -> "3" [label = b];
}
