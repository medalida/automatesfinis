digraph finite_state_machine {
  rankdir=LR;  
  size="8,5"  label="(a+b)*a(a+b)*"  node [shape = doublecircle]; "4" "3" "2";
  node [shape = circle];
  __I__ [label="", style=invis, width=0]
  __I__ -> "0"
  "0" -> "4" [label = a];
  "0" -> "1" [label = b];
  "4" -> "3" [label = a];
  "4" -> "2" [label = b];
  "3" -> "3" [label = a];
  "3" -> "2" [label = b];
  "2" -> "3" [label = a];
  "2" -> "2" [label = b];
  "1" -> "4" [label = a];
  "1" -> "1" [label = b];
}
